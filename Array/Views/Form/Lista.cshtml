@model FormModel
@{
    ViewData["Title"] = "Lista";
}
    <div class="text-center">
        <h2>Numero mayor</h2>
        <h3> @Model.Mayor </h3>
        <br />
        <h2>Numero menor</h2>
        <h3> @Model.Menor </h3>
    </div>

    <table class="table table-striped table-dark">
        <thead>
            <tr>
                <th scope="col"> Numeros ingresados</th>
                @foreach(int num in Model.Numeros)
                {
                    <th scope="col">Numero @num</th>
                }

            </tr>
        </thead>
        <tbody>

            @for (int i = 0; i < Model.Numeros.Count; i++)
            {
            <tr>
                <td>@Model.Numeros[i]</td>
                @foreach (int x in Model.Numeros)
                {
                    <td>@(Model.Numeros[i] + x)</td>
                }
                </tr>

            }


</tbody>
        </table>


